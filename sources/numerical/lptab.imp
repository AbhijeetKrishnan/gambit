template <class T>
gbtBasicFeasibleSolution<T> gbtLPTableau<T>::DualBFS() const
{
  gbtBasicFeasibleSolution<T> cbfs((T) 0);
  gbtVector<T> d(MinRow(),MaxRow());
  DualVector(d);

  for(int i=MinRow();i<=MaxRow();i++) {
    if(!Member(-i)) 
      cbfs.Define(-i,d[i]);
  }
  // gout << "\ndual: " << d;
  return cbfs;
}

