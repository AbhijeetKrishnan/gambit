template <class T>
void gbtLPTableau<T>::BigDump(gbtOutput &to)
{
  //   gbtTableau<T>::BigDump(to);

  to << "\nBasis:";
  basis.Dump(to);
  to << "\nMinCol(): " << MinCol(); 
  to << "\nMaxCol(): " << MaxCol(); 
  to << "\nLastlabel(): " << LastLabel(); 
  to << "\nNumRows(): " << (*A).NumRows(); 
  gbtMatrix<T> AA(MinRow(),MaxRow(),MinCol(),LastLabel()+(*A).NumRows());
  gbtVector<T> bb(MinRow(), MaxRow());

  BasisVector(bb);
  to << "\nBasisVector:\n" << bb;

  for(int j=MinCol();j<=LastLabel();j++) {
    SolveColumn(j, bb);
    for(int i=AA.MinRow();i<=AA.MaxRow();i++) 
      AA(i,j) = bb[i];
  }
  for(int j=MinRow();j<=MaxRow();j++) {
    SolveColumn(-j, bb);
    for(int i=AA.MinRow();i<=AA.MaxRow();i++) 
      AA(i,LastLabel()+j) = bb[i];
  }
  to << "\nTableau:\n" << AA;

  to << "\nCost vector: " << GetCost();
  to << "\nUnit Cost  : " << GetUnitCost();
  to << "\nTotal Cost: " << TotalCost();
}

template <class T>
void gbtLPTableau<T>::BasisSelect(const gbtBlock<T> &rowv, gbtVector<T> &colv) const
{
  for(int i=basis.First(); i<=basis.Last(); i++) {
    if(basis.Label(i)<0)
      colv[i]= 0;
    else
      colv[i]= rowv[basis.Label(i)];
  }
}

template <class T>
void gbtLPTableau<T>::BasisSelect(const gbtBlock<T> &unitv,
			   const gbtBlock<T> &rowv,
			   gbtVector<T> &colv ) const
{
  for(int i=basis.First(); i<=basis.Last(); i++) {
    if(basis.Label(i)<0)
      colv[i]= unitv[-basis.Label(i)];
    else
      colv[i]= rowv[basis.Label(i)];
  }
}

