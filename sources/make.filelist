ALLGUI_SOURCES = $(GLIB_SOURCES) $(GAME_SOURCES) $(GPOLY_SOURCES) \
                 $(ALG_SOURCES) $(GUI_SOURCES)

ALLGUI_OBJECTS = $(ALLGUI_SOURCES:.cc=$(OBJECT_SUFFIX))

SOURCES = $(GLIB_SOURCES) $(GAME_SOURCES) $(GPOLY_SOURCES) \
          $(ALG_SOURCES) $(GCL_SOURCES) $(GUI_SOURCES) \
          gclstatus.cc gio.cc gcl.cc wxgcl.cc

OBJECTS = $(SOURCES:.cc=$(OBJECT_SUFFIX))



