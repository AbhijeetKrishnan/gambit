GUI_SOURCES = gambit.cc treewin.cc twflash.cc treedraw.cc treedrag.cc \
              efglayout.cc wxmisc.cc splash.cc dlabout.cc efgshow.cc \
	      efgsolvd.cc nfgsolvd.cc dlefg.cc dlnfg.cc \
              efgsolng.cc wxstatus.cc algdlgs.cc \
              behavfilter.cc efgsoln.cc nfgshow.cc \
              nfgsoln.cc nfgsolng.cc mixedfilter.cc \
