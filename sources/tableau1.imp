void LPTableau<double>::SolveDual()
{
  gVector<double> tmpcol1(MinRow(),MaxRow());
  basis.BasisSelect(unitcost,cost,tmpcol1);
  // gout << "\nIn SolveDual() tab = \n";
  // Dump(gout);
  // gout << "\n tmpcol1: " << tmpcol1;
  B.solveT(tmpcol1,dual);
  // gout << "\n dual: " << dual;
}

    // Redefined functions
void LPTableau<double>::Refactor()
{
  Tableau<double>::Refactor();
  SolveDual();
}

void LPTableau<double>::Pivot(int outrow,int col)
{
  Tableau<double>::Pivot(outrow,col);
  SolveDual();
}

