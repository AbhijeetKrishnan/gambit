void Tableau<double>::BigDump(gOutput &to) const
{ 
  to << "\nBasis:\n";
  basis.Dump(to);
  gMatrix<double> AA((*A).MinRow(),(*A).MaxRow(),(*A).MinCol(),(*A).MaxCol()+(*A).NumRows());
  gVector<double> bb(*b);

  BasisVector(bb);
  to << "\nBasisVector:\n" << bb;

  for(int j=(*A).MinCol();j<=(*A).MaxCol();j++) {
    SolveColumn(j, bb);
    for(int i=AA.MinRow();i<=AA.MaxRow();i++) 
      AA(i,j) = bb[i];
  }
  for(int j=AA.MinRow();j<=AA.MaxRow();j++) {
    SolveColumn(-j, bb);
    for(int i=AA.MinRow();i<=AA.MaxRow();i++) 
      AA(i,(*A).MaxCol()+j) = bb[i];
  }
  to << "\nTableau:\n" << AA;
}

void Tableau<gRational>::BigDump(gOutput &to) const
{ 
  to << "\nBasis:\n";
  basis.Dump(to);
  gMatrix<double> AA((*A).MinRow(),(*A).MaxRow(),(*A).MinCol(),(*A).MaxCol()+(*A).NumRows());
  gVector<double> bb((*b).First(),(*b).Last());
  gVector<gRational> bbb(*b);

  BasisVector(bbb);
  bbb = bbb/denom;
  bbb *= sign(denom*totdenom);
  for(int i=bb.First();i<=bb.Last();i++) 
    bb[i]=bbb[i];
  to << "\nBasisVector:\n" << bb;

  for(int j=(*A).MinCol();j<=(*A).MaxCol();j++) {
    SolveColumn(j, bbb);
    bbb = bbb/denom;
    bbb *= sign(denom*totdenom);
    for(int i=AA.MinRow();i<=AA.MaxRow();i++) 
      AA(i,j) = bbb[i];
  }
  for(int j=AA.MinRow();j<=AA.MaxRow();j++) {
    SolveColumn(-j, bbb);
    bbb = bbb/denom;
    bbb *= sign(denom*totdenom);
    for(int i=AA.MinRow();i<=AA.MaxRow();i++) 
      AA(i,(*A).MaxCol()+j) = bbb[i];
  }
  to << "\nTableau:\n" << AA;
}

LPTableau<double>::LPTableau(const gMatrix<double> &A, const gVector<double> &b)
  : Tableau<double>(A,b), dual(A.MinRow(),A.MaxRow()),
    unitcost(A.MinRow(),A.MaxRow()),
    cost(A.MinCol(),A.MaxCol())
{
//  B.refactor(A, basis.label);
  B.solve(b, solution);
};


LPTableau<double>::LPTableau(const LPTableau<double> &orig)
  : Tableau<double>(orig), dual(orig.dual),  unitcost(orig.unitcost),
    cost(orig.cost)
{ }

LPTableau<double>::~LPTableau()
{ }


LPTableau<double>& LPTableau<double>::operator=(const LPTableau<double> &orig)
{
//  assert( A==orig.A );
  Tableau<double>::operator=(orig);
  if(this!= &orig) {
    dual=orig.dual;
    unitcost= orig.unitcost;
    cost= orig.cost;
  }
  return *this;
}

// cost-based functions

void LPTableau<double>::SetCost(const gVector<double>& c)
{
  int i;
  if(cost.Check(c)) {
    unitcost= (double)0;
    cost= c;
    SolveDual();
    return;
  }
  assert(c.First()==cost.First());
  assert(c.Last()==(cost.Last()+unitcost.Length()));
  for(i=c.First();i<=cost.Last();i++)
    cost[i]=c[i];
  for(i=unitcost.First();i<=unitcost.Last();i++)
    unitcost[i]=c[cost.Length()+i-unitcost.First()+1];

  SolveDual();
}

void LPTableau<double>::SetCost(const gVector<double> &uc, const gVector<double> &c)
{
  assert(unitcost.Check(uc) && cost.Check(c));
  unitcost= uc;
  cost= c;
  SolveDual();
}


gVector<double> LPTableau<double>::GetCost(void) const
{
  return cost; 
}


gVector<double> LPTableau<double>::GetUnitCost(void) const
{
  return unitcost; 
}


double LPTableau<double>::TotalCost()
{
//  gVector<double> tmpcol(MinRow(),MaxRow());
  basis.BasisSelect(unitcost,cost,tmpcol);
  return tmpcol*solution;
}


void LPTableau<double>::DualVector(gVector<double> &L) const
{
  L= dual;
}


double LPTableau<double>::RelativeCost(int col) const
{
  if( col<0 ) {
    return unitcost[-col] - dual[-col];
  }
  else {
//      gVector<double> tmpcol(MinRow(),MaxRow());
//    A->GetColumn(col, (gArray<double> &)tmpcol);
    basis.GetColumn(col, (gVector<double> &)tmpcol);
    return cost[col] - dual*tmpcol;
  }
}
