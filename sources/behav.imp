template <class T>
BehavAssessment<T>::BehavAssessment(const EFSupport &p_support)
  : BehavProfile<T>(p_support), m_beliefs(p_support.Game().NumMembers())
{
  m_beliefs = BehavProfile<T>::Beliefs();
}

